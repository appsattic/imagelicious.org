{
  "name": "imagelicious.org",
  "description": "Image hosting service, using only Firebase.",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "envify": "^3.4.1",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "firebase-tools": "^3.0.6",
    "hashirouter": "^1.1.0",
    "slugit": "^1.0.0",
    "uglify-js": "^2.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "comments": false
        }
      ],
      "envify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "firebase": "global:firebase",
    "react": "global:React",
    "react-dom": "global:ReactDOM"
  },
  "license": "ISC"
}
